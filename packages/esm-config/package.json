{
  "name": "@openmrs/esm-config",
  "version": "3.1.10",
  "license": "MPL-2.0",
  "description": "A configuration library for the OpenMRS Single-Spa framework.",
  "browser": "dist/openmrs-esm-module-config.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "document": "../../document.sh esm-config",
    "test": "jest --config jest.config.js --passWithNoTests",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "typescript": "tsc",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src/**"
  },
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ramda": "^0.26.1",
    "systemjs-webpack-interop": "^2.1.2"
  },
  "peerDependencies": {
    "@openmrs/esm-globals": "3.x",
    "@openmrs/esm-state": "3.x",
    "single-spa": "4.x",
    "systemjs": "6.x"
  },
  "devDependencies": {
    "@openmrs/esm-globals": "^3.1.10",
    "@openmrs/esm-state": "^3.1.10",
    "@types/ramda": "^0.26.44",
    "@types/systemjs": "^6.1.0",
    "babel-plugin-ramda": "^2.0.0",
    "single-spa": "^4.4.1"
  }
}

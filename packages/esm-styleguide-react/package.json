{
  "name": "@openmrs/esm-styleguide-react",
  "version": "3.1.10",
  "description": "Library of custom reusable React components for OpenMRS SPA",
  "browser": "dist/openmrs-esm-styleguide-react.js",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MPL-2.0",
  "homepage": "https://github.com/openmrs/openmrs-esm-home#readme",
  "scripts": {
    "start": "openmrs debug --run-project",
    "serve": "webpack-dev-server",
    "debug": "npm run serve",
    "build": "webpack --mode production",
    "lint": "eslint src --ext tsx",
    "prettier": "prettier 'src/**/*' --write",
    "typescript": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "coverage": "npm test -- --coverage",
    "start:storybook": "start-storybook --port 7000",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'npm:test' 'npm:typescript' 'npm:lint'"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-home.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-home/issues"
  },
  "dependencies": {
    "carbon-components-react": "^7.24.0",
    "lodash-es": "^4.17.15",
    "systemjs-webpack-interop": "^1.1.0"
  },
  "peerDependencies": {
    "@openmrs/esm-styleguide": "3.x",
    "@openmrs/esm-react-utils": "3.x",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.20",
    "@testing-library/jest-dom": "^5.11.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "jest": "^26.0.1",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^11.7.2",
    "sass": "^1.29.0",
    "sass-loader": "10",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.43.0"
  }
}

{
  "name": "@openmrs/esm-styleguide",
  "version": "5.1.0",
  "license": "MPL-2.0",
  "description": "The styleguide for OpenMRS SPA",
  "browser": "dist/openmrs-esm-styleguide.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "test": "cross-env TZ=UTC jest --config jest.config.js --verbose false --passWithNoTests --color",
    "test:watch": "cross-env TZ=UTC jest --watch --config jest.config.js --color",
    "start:storybook": "start-storybook --port 7000",
    "build:storybook": "build-storybook",
    "build": "webpack --mode=production",
    "build:development": "webpack --mode development",
    "analyze": "webpack --mode=production --env analyze=true",
    "lint": "eslint src"
  },
  "keywords": [
    "openmrs",
    "microfrontends",
    "styleguide"
  ],
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@carbon/charts": "^1.6.3",
    "@carbon/react": "^1.12.0",
    "@internationalized/date": "^3.2.0",
    "@react-spectrum/datepicker": "^3.5.0",
    "@react-spectrum/provider": "^3.7.1",
    "@react-spectrum/theme-default": "^3.5.1",
    "d3": "^7.8.0",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "@openmrs/esm-extensions": "5.x",
    "@openmrs/esm-react-utils": "5.x",
    "@openmrs/esm-state": "5.x",
    "react": "18.x",
    "react-dom": "18.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@openmrs/esm-extensions": "^5.1.0",
    "@openmrs/esm-react-utils": "^5.1.0",
    "@openmrs/esm-state": "^5.1.0",
    "autoprefixer": "^9.8.8",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "i18next": "^19.6.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "postcss": "^8.4.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rxjs": "^6.5.3"
  }
}

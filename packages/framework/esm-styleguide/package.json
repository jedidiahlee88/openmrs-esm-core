{
  "name": "@openmrs/esm-styleguide",
  "version": "4.0.0",
  "license": "MPL-2.0",
  "description": "The styleguide for OpenMRS SPA",
  "browser": "dist/openmrs-esm-styleguide.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "test": "jest --config jest.config.js --passWithNoTests",
    "start:storybook": "start-storybook --port 7000",
    "build:storybook": "build-storybook",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze=true",
    "lint": "eslint src"
  },
  "keywords": [
    "openmrs",
    "microfrontends",
    "styleguide"
  ],
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@carbon/react": "^1.3.0",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "@openmrs/esm-extensions": "4.x",
    "@openmrs/esm-react-utils": "4.x",
    "@openmrs/esm-state": "4.x",
    "react": "18.x",
    "react-dom": "18.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@openmrs/esm-extensions": "^4.0.0",
    "@openmrs/esm-react-utils": "^4.0.0",
    "@openmrs/esm-state": "^4.0.0",
    "autoprefixer": "^9.8.8",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "postcss": "^8.4.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rxjs": "^6.5.3"
  }
}

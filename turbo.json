{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:development": {
      "dependsOn": ["^build:development"],
      "outputs": ["dist/**"]
    },
    "document": {
      "outputs": ["docs/**"]
    },
    "test": {},
    "test:watch": {},
    "watch": {},
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typescript": {
      "dependsOn": ["^typescript"]
    },
    "extract-translations": {}
  }
}

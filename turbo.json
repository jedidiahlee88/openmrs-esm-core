{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "watch": {
      "dependsOn": [],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "typescript": {
      "dependsOn": ["^typescript"],
      "outputs": []
    },
    "extract-translations": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
